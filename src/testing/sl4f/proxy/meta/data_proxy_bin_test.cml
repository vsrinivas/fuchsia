{
    include: [
        "//src/sys/test_runners/rust/default.shard.cml",
        "syslog/client.shard.cml",
    ],
    program: {
        binary: "bin/data_proxy_bin_test",
    },
    children: [
        {
            name: "netstack-debug",
            url: "#meta/netstack-debug.cm",
        },
    ],
    use: [
        {
            protocol: [ "fuchsia.posix.socket.Provider" ],
            from: "#netstack-debug",
        },
    ],
    offer: [
        {
            protocol: [ "fuchsia.logger.LogSink" ],
            from: "parent",
            to: [ "#netstack-debug" ],
        },
    ],
}
