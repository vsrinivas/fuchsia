library test.security.pkg;

const MAX_STRING_SIZE uint64 = 30;

/// Fetches metadata regarding a local package server.
@discoverable
protocol PackageServer {
    /// Gets the URL of the local package server.
    GetUrl() -> (struct {
        url string:MAX_STRING_SIZE;
    });
};
