import("//build/rust/rustc_library.gni")

group("rust") {
  public_deps = [ ":tee" ]
}

rustc_library("tee") {
  edition = "2021"
  deps = [ "//third_party/rust_crates:tracing" ]
  non_rust_deps = [ "//src/security/tee/tee-client-api" ]
  sources = [
    "src/lib.rs",
    "src/tee_client_api.rs",
  ]
}
