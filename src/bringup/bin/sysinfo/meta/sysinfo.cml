{
    program: {
        binary: "bin/sysinfo",
    },
    capabilities: [
        { protocol: "fuchsia.sysinfo.SysInfo" },
    ],
    use: [
        { runner: "elf" },
        {
            directory: "dev",
            rights: [ "rw*" ],
            path: "/dev",
        },
        { protocol: "fuchsia.boot.WriteOnlyLog" },
    ],
    expose: [
        {
            protocol: "fuchsia.sysinfo.SysInfo",
            from: "self",
        },
    ],
}
