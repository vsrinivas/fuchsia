import("//build/test.gni")

group("test") {
  testonly = true
  deps = [ ":firmware_zbi_test($host_toolchain)" ]
}

test("firmware_zbi_test") {
  sources = [ "zbi.cc" ]
  deps = [
    "//src/firmware/lib/zbi:in_tree",
    "//zircon/public/lib/pretty",
    "//zircon/system/ulib/fbl",
    "//zircon/system/ulib/zxtest",
  ]
}
