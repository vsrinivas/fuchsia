{
    select: {
        total: "global_data:root.stats:total_bytes",
        used: "global_data:root.stats:used_bytes",
        foo_health: "foo.cmx:root.Health:status",
    },
    eval: {
        c1: "other::m == 1", // JSON5: Comments work, don't need so many quotes.
        c2: "other::m > 1",
        ratio: "used / (total + 0.0)",
        small: "ratio > 0.001",
        tiny: "ratio > 0.00000001",
        not_healthy: "foo_health != 'OK'",
    },
    act: {
        act2: {
            trigger: "c2",
            print: "yes on A!",
        },
        some_disk: {
            trigger: "tiny",
            print: "Used some of disk",
        },
        more_disk: {
            trigger: "small",
            print: "Used more of the disk",
        },
        unhealthy: {
            trigger: "not_healthy",
            print: "foo is not healthy",
        },
    },
    test: {
        trial1: {
            yes: [
                "some_disk",
                "more_disk",
            ],
            no: [],
            inspect: [
                {
                    path: "global_data",
                    contents: {
                        root: {
                            stats: {
                                total_bytes: 10,
                                used_bytes: 9,
                            },
                        },
                    },
                },
            ],
        },
        trial2: {
            yes: [
                "unhealthy",
            ],
            no: [],
            inspect: [
                {
                    path: "foo.cmx",
                    contents: {
                        root: {
                            Health: {
                                status: "UNHEALTHY",
                            },
                        },
                    },
                },
            ],
        },
    },
}
