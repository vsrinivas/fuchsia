library fuchsia.mockrebootcontroller;

type RebootError = strict enum : int32 {
    CLIENT_ERROR = 1;
    NO_CLIENT_SET = 2;
};

@discoverable
protocol MockRebootController {
    TriggerReboot() -> () error RebootError;
    CrashRebootChannel() -> () error RebootError;
};
