{
    "include": [
        "sdk/lib/diagnostics/syslog/client.shard.cmx"
    ],
    "program": {
        "args": [
            "--disconnect-secs",
            "1800",
            "--rebind-probability",
            "0.1"
        ],
        "binary": "bin/stressor"
    },
    "sandbox": {
        "features": [
            "hub"
        ],
        "services": [
            "fuchsia.boot.Arguments",
            "fuchsia.boot.Items",
            "fuchsia.boot.WriteOnlyLog",
            "fuchsia.ldsvc.Loader",
            "fuchsia.process.Launcher",
            "fuchsia.sys.Environment",
            "fuchsia.sys.Launcher"
        ]
    }
}
