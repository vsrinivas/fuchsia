group("fs-mkfs") {
  deps = [ ":mkfs" ]
}

executable("mkfs") {
  configs += [ "//build/unification/config:zircon-migrated" ]
  sources = [ "main.cc" ]
  deps = [
    "//sdk/lib/fdio",
    "//sdk/lib/syslog/cpp:backend_legacy",
    "//zircon/public/lib/safemath",
    "//zircon/system/ulib/fs-management",
  ]
}
