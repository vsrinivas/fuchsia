{
    include: [
        "//src/sys/test_runners/rust/default.shard.cml",
        "syslog/client.shard.cml",
    ],
    program: {
        binary: "bin/fuchsia_async_lib_test",
    },
    children: [
        {
            name: "netstack",
            url: "#meta/netstack.cm",
        },
    ],
    use: [
        {
            protocol: [ "fuchsia.posix.socket.Provider" ],
            from: "#netstack",
        },
    ],
    offer: [
        {
            protocol: [ "fuchsia.logger.LogSink" ],
            from: "parent",
            to: "#netstack",
        },
        {
            storage: "cache",
            from: "parent",
            to: [ "#netstack" ],
        },
    ],
}
