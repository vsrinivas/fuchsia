library test.fidlcodec.examples;

protocol FidlCodecXUnion {
    SendAfterMigration(struct {
        u NowAsXUnion;
        i int32;
    });
};

type NowAsXUnion = flexible union {
    1: variant_u8 uint8;
    2: variant_u16 uint16;
};

type OriginalUnion = strict union {
    1: variant_u8 uint8;
    2: variant_u16 uint16;
};
