// This realm contains the test cases for fuchsia_component_v2_test.
{
    program: {
        binary: "bin/realm",
    },
    collections: [
        {
            name: "coll",
            durability: "transient",
        },
    ],
    use: [
        { runner: "elf" },
        { protocol: "/svc/fuchsia.logger.LogSink" },
        {
            // Needed to instantiate dynamic components.
            protocol: "/svc/fuchsia.sys2.Realm",
            from: "framework",
        },
        {
            event: "destroyed",
            from: "framework",
        },
    ],
}
