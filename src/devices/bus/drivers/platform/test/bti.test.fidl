library fuchsia.hardware.btitest;

@discoverable
protocol BtiDevice {
    /// Returns koid for BTI retrieved from platform bus.
    GetKoid() -> (struct {
        koid uint64;
    });
    /// Crashes driver host.
    Crash();
};
