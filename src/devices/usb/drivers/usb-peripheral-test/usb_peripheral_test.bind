using fuchsia.usb;
using fuchsia.google.usb;

fuchsia.BIND_PROTOCOL == fuchsia.usb.BIND_PROTOCOL.FUNCTION;
fuchsia.BIND_USB_VID == fuchsia.google.usb.BIND_USB_VID.GOOGLE;
accept fuchsia.BIND_USB_PID {
  fuchsia.google.usb.BIND_USB_PID.FUNCTION_TEST,
  fuchsia.google.usb.BIND_USB_PID.CDC_AND_FUNCTION_TEST,
}
