{
    include: [
        "inspect/client.shard.cml",
        "syslog/client.shard.cml",
    ],
    program: {
        runner: "driver",
        binary: "driver/runtime-dispatcher-root.so",
        bind: "meta/bind/root.bindbc",
    },
    capabilities: [
        { protocol: "fuchsia.runtime.test.Handshake" },
    ],
    expose: [
        {
            protocol: "fuchsia.runtime.test.Handshake",
            from: "self",
        },
    ],
}
