const strings_size uint32 = 32;

type strings = struct {
    s string;
    nullable_s string:optional;
    size_0_s string:4;
    size_1_s string:strings_size;
    nullable_size_0_s string:<4, optional>;
    nullable_size_1_s string:<strings_size, optional>;
};

