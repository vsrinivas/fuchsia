library banjo.examples.types;

using zx;

type this_is_a_struct = struct {
    s string;
};

type this_is_a_union = strict union {
    1: s string;
};

@transport("Banjo")
protocol this_is_an_interface {
    @some_attrib
    @another_attrib("Value containing, a comma")
    Copy(struct {
        s string;
        count uint32;
    }) -> (struct {
        s string;
    });
};

type interfaces = resource struct {
    nonnullable_interface client_end:this_is_an_interface;
    nullable_interface client_end:<this_is_an_interface, optional>;
};

type primitive_types = struct {
    b bool;
    i8 int8;
    i16 int16;
    i32 int32;
    i64 int64;
    u8 uint8;
    u16 uint16;
    u32 uint32;
    u64 uint64;
    f32 float32;
    f64 float64;
};

type zx_types = struct {
    status zx.status;
    time zx.time;
    duration zx.duration;
    ticks zx.ticks;
    koid zx.koid;
    vaddr zx.vaddr;
    paddr zx.paddr;
    paddr32 zx.paddr32;
    gpaddr zx.gpaddr;
    offset zx.off;
    procarg zx.procarg;
    signals zx.signals;
};

type default_values = struct {
    b1 bool = true;
    b2 bool = false;
    i8 int8 = -23;
    i16 int16 = 34;
    i32 int32 = -34595;
    i64 int64 = 3948038;
    u8 uint8 = 0;
