import("//build/config.gni")

group("policy") {
  deps = [ ":configs" ]
}
group("configs") {
  deps = [ ":ui_policy_config" ]
}

config_data("ui_policy_config") {
  for_pkg = "sysmgr"
  outputs = [ "ui_policy.config" ]
  sources = [ "service.config" ]
}
