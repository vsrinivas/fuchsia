    bindings_denylist = [llcpp],
    value = PaddingBetweenFieldsInt8Int16{
        a: 0x01,
        b: 0x0203,
    },
    bytes = {
        v1 = [
            0x01, 0x00, 0x03, 0x02, 0x00, 0x00, 0x00, 0x00,
        ]
    }
}

