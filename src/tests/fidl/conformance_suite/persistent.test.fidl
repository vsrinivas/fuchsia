library conformance;

type Persistent1Message = struct {
    header PersistentHeader;
    body Table_PersistentMessage;
};

type PersistentHeader = struct {
    flags array<uint8, 3>;
    magic_number uint8;
};

type Table_PersistentMessage = table {
    1: i uint32;
};
