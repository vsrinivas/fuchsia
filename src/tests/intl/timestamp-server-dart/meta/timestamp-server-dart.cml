{
    include: [
        // Enable system logging.
        "syslog/client.shard.cml",
    ],
    program: {
        binary: "data/timestamp-server-dart",
    },
    capabilities: [
        {
            protocol: [ "test.placeholders.Echo" ],
        },
    ],
    use: [
        {
            protocol: [ "fuchsia.intl.PropertyProvider" ],
        },
    ],
    expose: [
        {
            protocol: [ "test.placeholders.Echo" ],
            from: "self",
        },
    ],
}
