{
    include: [ "//src/virtualization/bin/guest_manager/meta/guest_manager.cml" ],
    program: {
        runner: "elf",
        binary: "bin/guest_manager",
    },
    children: [
        {
            name: "guest_pkg",
            url: "fuchsia-pkg://fuchsia.com/debian_guest#meta/debian_guest.cm",
        },
    ],
    expose: [
        {
            protocol: "fuchsia.virtualization.GuestManager",
            from: "self",
            as: "fuchsia.virtualization.DebianGuestManager",
        },
    ],
}
