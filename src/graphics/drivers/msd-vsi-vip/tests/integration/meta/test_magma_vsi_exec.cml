    include: [
        "//src/sys/test_runners/gtest/default.shard.cml",
        "syslog/client.shard.cml",
    ],
    program: {
        binary: "test/test_magma_vsi_exec_bin",
    },
    use: [
        {
            directory: "dev-topological",
            rights: [ "r*" ],
            path: "/dev",
