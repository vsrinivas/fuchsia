{
    "include": [
        "sdk/lib/diagnostics/syslog/client.shard.cmx"
    ],
    "program": {
        "binary": "bin/exceptions"
    },
    "sandbox": {
        "features": [
            "config-data",
            "isolated-temp"
        ],
        "services": [
            "fuchsia.feedback.CrashReporter",
            "fuchsia.process.Launcher",
            "fuchsia.sys.internal.CrashIntrospect"
        ]
    }
}
