{
    "facets": {
        "fuchsia.test": {
            "system-services": [
                "fuchsia.kernel.Stats"
            ]
        }
    },
    "include": [
        "sdk/lib/diagnostics/syslog/client.shard.cmx"
    ],
    "program": {
        "binary": "bin/memory_monitor_test_app"
    },
    "sandbox": {
        "features": [
            "isolated-cache-storage"
        ],
        "services": [
            "fuchsia.kernel.Stats",
            "fuchsia.tracing.provider.Registry"
        ]
    }
}
