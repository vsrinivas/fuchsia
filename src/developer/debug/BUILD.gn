group("debug") {
  testonly = true

  deps = [
    "debug_agent",
    "ipc",
    "shared",
    "unwinder/benchmark($host_toolchain)",
  ]
}

group("tests") {
  testonly = true

  deps = [
    "debug_agent:tests",
    "e2e_tests:tests",
    "zxdb:tests",
  ]
}
