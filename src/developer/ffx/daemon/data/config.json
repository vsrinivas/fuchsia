{
  "overnet": {
    "socket": "/tmp/ascendd",
    "max_retry_count": 30
  },
  "ssh": {
    "port": "$FUCHSIA_SSH_PORT",
    "priv": [
      "$FUCHSIA_SSH_KEY",
      "$HOME/.ssh/fuchsia_ed25519",
      "$FUCHSIA_DIR/.ssh/pkey"
    ],
    "pub": [
      "$FUCHSIA_AUTHORIZED_KEYS",
      "$HOME/.ssh/fuchsia_authorized_keys",
      "$FUCHSIA_DIR/.ssh/authorized_keys"
    ]
  },
  "events": {
    "timeout_secs": 10
  }
}
