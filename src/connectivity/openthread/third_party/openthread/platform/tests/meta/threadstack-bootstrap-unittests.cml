{
    include: [
        "//src/sys/test_runners/gtest/default.shard.cml",
        "syslog/client.shard.cml",
    ],
    program: {
        binary: "bin/threadstack-bootstrap-unittests",
    },
    children: [
        {
            name: "netstack",
            url: "#meta/netstack.cm",
        },
    ],
    use: [
        {
            protocol: [ "fuchsia.posix.socket.Provider" ],
            from: "#netstack",
        },
        {
            storage: "data",
            path: "/data",
        },
    ],
}
