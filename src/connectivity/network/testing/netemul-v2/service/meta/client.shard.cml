{
    children: [
        {
            name: "sandbox",
            url: "#meta/netemul-sandbox-v2.cm",
        },
    ],
    use: [
        {
            protocol: [ "fuchsia.netemul.Sandbox" ],
            from: "#sandbox",
        },
    ],
    offer: [
        {
            protocol: [
                "fuchsia.logger.LogSink",
                "fuchsia.process.Launcher",
            ],
            from: "parent",
            to: [ "#sandbox" ],
        },
    ],
}
