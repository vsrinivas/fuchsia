{
    "program": {
        "args": [
            "netstack",
            "--log-packets",
            "--verbosity=debug"
        ],
        "binary": "bin/grand_unified_binary"
    },
    "sandbox": {
        "features": [
            "isolated-cache-storage"
        ],
        "services": [
            "fuchsia.cobalt.LoggerFactory",
            "fuchsia.device.NameProvider",
            "fuchsia.logger.LogSink",
            "fuchsia.posix.socket.Provider",
            "fuchsia.stash.SecureStore"
        ]
    }
}
