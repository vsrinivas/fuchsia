{
    include: [
        "//sdk/lib/diagnostics/inspect/client.shard.cml",
        "syslog/client.shard.cml",
    ],
    program: {
        runner: "elf",
        binary: "bin/stash",
        args: [
            "--secure",
            "--backing_file",
            "/data/stash_secure.store",
        ],
    },
    capabilities: [
        { protocol: "fuchsia.stash.SecureStore" },
    ],
    use: [
        {
            storage: "temp",
            path: "/data",
        },
    ],
    expose: [
        {
            protocol: "fuchsia.stash.SecureStore",
            from: "self",
        },
    ],
}
