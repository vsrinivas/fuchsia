import("//src/sys/build/components.gni")

fuchsia_shell_package("runmany") {
  deps = [ ":runmany_bin" ]
}

executable("runmany_bin") {
  output_name = "runmany"
  sources = [ "src/runmany.c" ]
  deps = [
    "//sdk/lib/fdio",
    "//zircon/public/lib/zx",
  ]
}
