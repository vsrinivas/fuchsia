{
    "program": {
        "binary": "bin/netclock"
    },
    "sandbox": {
        "features": [
            "build-info"
        ],
        "services": [
            "fuchsia.logger.LogSink",
            "fuchsia.net.Connectivity",
            "fuchsia.net.NameLookup",
            "fuchsia.net.SocketProvider",
            "fuchsia.posix.socket.Provider",
            "fuchsia.timezone.TimeService"
        ]
    }
}
