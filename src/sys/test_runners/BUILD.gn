import("//build/rust/rustc_library.gni")

rustc_library("lib") {
  name = "test_runners_lib"
  with_unit_tests = true
  edition = "2018"

  # Only for internal use.
  visibility = [
    "gtest/*",
    "rust/*",
    ":*",
  ]

  deps = []
}

group("test_runners") {
  testonly = true
  deps = [
    "gtest",
  ]
}

group("tests") {
  testonly = true
  deps = [
    "gtest",
  ]
}
