{
    include: [ "sdk/lib/diagnostics/syslog/client.shard.cml" ],
    program: {
        binary: "bin/test_thinger",
    },
    capabilities: [
        { protocol: "test.pkg.thinger.Thinger" },
    ],
    use: [
        { runner: "elf" },
        {
            directory: "pkgfs",
            rights: [ "rw*" ],
            path: "/pkgfs",
        },
    ],
    expose: [
        {
            protocol: "test.pkg.thinger.Thinger",
            from: "self",
        },
    ],
}
