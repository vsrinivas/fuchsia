{
    children: [
        {
            name: "pkgfs_reflector",
            url: "fuchsia-pkg://fuchsia.com/dir-reflector-integration-test#meta/pkgfs-reflector.cm",
        },
        {
            name: "thinger",
            url: "fuchsia-pkg://fuchsia.com/dir-reflector-integration-test#meta/test-case-thinger.cm",
        },
    ],
    offer: [
        {
            protocol: "fuchsia.logger.LogSink",
            from: "parent",
            to: [
                "#pkgfs_reflector",
                "#thinger",
            ],
        },
        {
            directory: "pkgfs",
            from: "#pkgfs_reflector",
            as: "pkgfs",
            to: [ "#thinger" ],
        },
    ],
    expose: [
        {
            protocol: "test.pkg.reflector.Reflector",
            from: "#pkgfs_reflector",
        },
        {
            directory: "pkgfs",
            from: "#pkgfs_reflector",
        },
        {
            protocol: "test.pkg.thinger.Thinger",
            from: "#thinger",
        },
    ],
}
