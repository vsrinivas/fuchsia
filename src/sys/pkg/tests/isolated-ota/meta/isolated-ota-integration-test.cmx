{
    "facets": {
        "fuchsia.test": {
            "injected-services": {
                "fuchsia.net.name.Lookup": "fuchsia-pkg://fuchsia.com/dns-resolver#meta/dns-resolver.cmx",
                "fuchsia.net.routes.State": "fuchsia-pkg://fuchsia.com/netstack#meta/netstack.cmx",
                "fuchsia.posix.socket.Provider": "fuchsia-pkg://fuchsia.com/netstack#meta/netstack.cmx",
                "fuchsia.test.IsolatedDevmgr": "fuchsia-pkg://fuchsia.com/ramdisk-isolated-devmgr#meta/ramdisk-isolated-devmgr.cmx"
            }
        }
    },
    "include": [
        "//src/lib/fuchsia-hyper/hyper.shard.cmx",
        "syslog/client.shard.cmx"
    ],
    "program": {
        "binary": "bin/isolated_ota_integration_test"
    },
    "sandbox": {
        "dev": [
            "sys/platform/00:00:2d/ramctl"
        ],
        "features": [
            "isolated-temp",
            "root-ssl-certificates"
        ],
        "services": [
            "fuchsia.process.Launcher",
            "fuchsia.sys.Environment",
            "fuchsia.sys.Launcher",
            "fuchsia.sys.Loader",
            "fuchsia.test.IsolatedDevmgr",
            "fuchsia.tracing.provider.Registry"
        ]
    }
}
