library fuchsia.elf.test;

const MAX_ENV_VARS int32 = 32;
const MAX_VAR_LEN int32 = 132;

@discoverable
protocol Context {
    /// Returns the environment set for this component.
    GetEnviron() -> (struct {
        environ vector<string:MAX_VAR_LEN>:MAX_ENV_VARS;
    });
};
