{
    collections: [
        {
            name: "dynamic_children",
            durability: "transient",
        },
    ],
    offer: [
        {
            protocol: "fidl.examples.routing.echo.Echo",
            from: "parent",
            to: "#dynamic_children",
        },
        {
            protocol: "fuchsia.logger.LogSink",
            from: "parent",
            to: "#dynamic_children",
        },
    ],
    expose: [
        {
            directory: "hub",
            from: "framework",
            rights: [ "rw*" ],
        },
        {
            protocol: "fuchsia.sys2.RealmExplorer",
            from: "framework",
        },
    ],
}
