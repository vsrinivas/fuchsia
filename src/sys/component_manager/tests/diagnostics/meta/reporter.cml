{
    include: [ "//sdk/lib/diagnostics/syslog/client.shard.cml" ],
    program: {
        runner: "elf",
        binary: "bin/reporter",
    },
    use: [
        {
            directory: "hub",
            from: "parent",
            rights: [ "r*" ],
            path: "/hub",
        },
        {
            protocol: [
                "fidl.examples.routing.echo.Echo",
                "fuchsia.diagnostics.ArchiveAccessor",
            ],
        },
    ],
}
