{
    include: [ "syslog/client.shard.cml" ],
    program: {
        runner: "elf",
        binary: "bin/security-policy-create-raw-processes-test",
        job_policy_create_raw_processes: "true",
        is_shared_process: "true",
    },
    capabilities: [
        { protocol: "fuchsia.test.Suite" },
    ],
    expose: [
        {
            protocol: "fuchsia.test.Suite",
            from: "self",
        },
    ],
    facets: {
        "fuchsia.test": { type: "system" },
    },
}
