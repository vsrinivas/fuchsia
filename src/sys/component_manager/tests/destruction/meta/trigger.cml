// Integration test component that runs a Trigger service. When triggered, the
// component is started but takes no action.
{
    "program": {
        "binary": "bin/trigger",
    },
    "use": [
        { "runner": "elf" }
    ],
    "expose": [
        {
            "protocol": "/svc/fidl.test.components.Trigger",
            "from": "self",
        },
    ],
}
