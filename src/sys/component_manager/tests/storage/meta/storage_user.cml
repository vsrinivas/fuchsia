{
    "program": {
        "binary": "bin/storage_user",
    },
    "use": [
        { "runner": "elf" },
        { "storage": "data" },
        { "protocol": "/svc/fuchsia.logger.LogSink" },
    ],
    "expose": [
        {
            "directory": "/data",
            "from": "self",
            "rights": ["rw*"],
        }
    ],
}
