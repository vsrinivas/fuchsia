{
    "storage": [
        {
            "name": "mystorage",
            "from": "#memfs",
            "path": "/memfs",
        },
    ],
    "offer": [
        {
            "storage": "data",
            "from": "#mystorage",
            "to": [
                { "dest": "#storage_user" },
                { "dest": "#coll" },
            ],
        },
        {
            "legacy_service": "/svc/fuchsia.logger.LogSink",
            "from": "realm",
            "to": [
                { "dest": "#storage_user" },
                { "dest": "#coll" },
                { "dest": "#memfs" },
            ]
        },
    ],
    "children": [
        {
            "name": "memfs",
            "url": "fuchsia-pkg://fuchsia.com/storage_integration_test#meta/memfs.cm",
        },
        {
            "name": "storage_user",
            "url": "fuchsia-pkg://fuchsia.com/storage_integration_test#meta/storage_user.cm",
        },
    ],
    "collections": [
        {
            "name": "coll",
            "durability": "transient",
        },
    ],
}
