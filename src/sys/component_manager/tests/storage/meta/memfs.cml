{
    program: {
        binary: "bin/memfs",
    },
    capabilities: [
        {
            directory: "memfs",
            rights: [ "rw*" ],
            path: "/svc/fuchsia.io.Directory",
        },
    ],
    use: [
        { runner: "elf" },
        { protocol: "/svc/fuchsia.logger.LogSink" },
    ],
    expose: [
        {
            directory: "memfs",
            from: "self",
        },
    ],
}
