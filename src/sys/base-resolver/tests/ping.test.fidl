library test.ping;

using PingString = string:100;
using PongString = string:105;

[Discoverable]
protocol Ping {
    /// Returns a string that is the concatenation of the input `ping`
    /// and the string " pong".
    Ping(PingString ping) -> (PongString pong);
};
