import("//build/test.gni")
import("//build/unification/zbi/bootfs_test.gni")

test("processor") {
  output_name = "processor-test"
  configs += [ "//build/unification/config:zircon-migrated" ]
  sources = [ "processor.c" ]
  deps = [
    "//sdk/lib/fdio",
    "//zircon/public/lib/zxtest",
  ]
}

bootfs_test("processor-bootfs-test") {
  name = "processor-test"
  deps = [ ":processor" ]
}
