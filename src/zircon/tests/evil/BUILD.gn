executable("evil") {
  configs += [ "//build/unification/config:zircon-migrated" ]
  output_name = "evil-tests"
  sources = [ "evil.c" ]
  deps = [ "//sdk/lib/fdio" ]
  libs = [ "zircon" ]

  # TODO(fxb/58162): delete the below and fix compiler warnings
  configs += [ "//build/config:Wno-conversion" ]
}
