executable("ktrace") {
  if (is_fuchsia) {
    configs += [ "//build/unification/config:zircon-migrated" ]
  }
  sources = [ "ktrace.cc" ]
  deps = [
    "//sdk/fidl/fuchsia.tracing.kernel:fuchsia.tracing.kernel_c",
    "//sdk/lib/fdio",
    "//zircon/public/lib/fbl",
    "//zircon/public/lib/zx",
  ]
}
