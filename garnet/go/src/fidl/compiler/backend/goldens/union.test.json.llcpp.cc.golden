    }
    case kTestProtocol_FlexibleXUnionHenceResponseMustBeHeapAllocated_Ordinal: {
      auto result = ::fidl::DecodeAs<
          FlexibleXUnionHenceResponseMustBeHeapAllocatedRequest>(msg);
      if (result.status != ZX_OK) {
