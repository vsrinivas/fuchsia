import("//build/package.gni")

executable("bin") {
  output_name = "uname"

  sources = [ "uname.c" ]

  deps = [ "//src/zircon/lib/zircon" ]
}

package("uname") {
  deps = [ ":bin" ]

  binaries = [
    {
      name = "uname"
      shell = true
    },
  ]
}
