library fuchsia.example.fostr;

bits ExampleBits {
    A = 1;
    B = 2;
    C = 4;
};

enum ExampleEnum {
    FOO = 1;
    BAR_BAZ = 2;
};

struct MyStruct {
    vector<int32>:10? nums;
    string foo;
    MyXunion bar;
};

xunion MyXunion {
    1: int32 i;
    2: bool b;
    3: reserved;
};

union MyUnion {
    1: int32 i;
    2: bool b;
    3: reserved;
};

table SimpleTable {
    1: bool x;
    2: reserved;
    3: MyStruct y;
    4: int32 z;
};
