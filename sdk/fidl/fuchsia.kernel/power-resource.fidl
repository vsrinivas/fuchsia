library fuchsia.kernel;

using zx;

/// Protocol for providing a `ZX_RSRC_KIND_SYSTEM` with base
/// `ZX_RSRC_SYSTEM_POWER_BASE`.
@discoverable
protocol PowerResource {
    /// Get a power resource handle as `power_resource`.
    Get() -> (resource struct {
        power_resource zx.handle:RESOURCE;
    });
};
