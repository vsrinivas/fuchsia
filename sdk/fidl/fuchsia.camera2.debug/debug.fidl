library fuchsia.camera2.debug;

using zx;

/// This is the interface to the camera driver
/// which allows debug settings.
@discoverable
protocol Debug {
    // Set the default sensor mode
    SetDefaultSensorMode(resource struct {
        mode uint32;
    }) -> (struct {
        status zx.status;
    });
};
