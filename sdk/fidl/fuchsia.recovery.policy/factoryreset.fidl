library fuchsia.recovery.policy;

/// A protocol for determining whether factory reset can be triggered.
@discoverable
protocol FactoryReset {
    /// A hanging-get style api that returns whether local factory reset is
    /// enabled.
    GetEnabled() -> (struct {
        fdr_enabled bool;
    });
};
