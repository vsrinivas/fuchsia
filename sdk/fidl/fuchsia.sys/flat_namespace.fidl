library fuchsia.sys;

using zx;

resource struct FlatNamespace {
  /// The mount point for each of the directories below.
  ///
  /// For example, ["/pkg", "/svc"].
  vector<string> paths;

  /// The directories mounted at each path in the namespace.
  vector<zx.handle:CHANNEL> directories;
};
