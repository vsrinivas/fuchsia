library fuchsia.sys;

using zx;

type FlatNamespace = resource struct {
    /// The mount point for each of the directories below.
    ///
    /// For example, ["/pkg", "/svc"].
    paths vector<string>;

    /// The directories mounted at each path in the namespace.
    directories vector<zx.handle:CHANNEL>;
};
