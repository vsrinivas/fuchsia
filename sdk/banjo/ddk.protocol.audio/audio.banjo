library ddk.protocol.audio;
using zx;

[Layout = "ddk-protocol"]
protocol Codec {
    /// This method connects a channel in order to serve FIDL over it outside the driver host
    /// control.
    Connect(handle<channel> channel) -> (zx.status status);
};
