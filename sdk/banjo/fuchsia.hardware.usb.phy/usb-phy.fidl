library fuchsia.hardware.usb.phy;

@transport("Banjo")
@banjo_layout("ddk-protocol")
protocol UsbPhy {
    /// Informs the PHY when peripheral role connect status changed.
    ConnectStatusChanged(struct {
        connected bool;
    });
};
