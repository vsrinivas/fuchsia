namespace modular;

enum IdentityProvider : byte { DEV, GOOGLE }

table UserStorage {
  id: string;
  identity_provider: IdentityProvider;
  display_name: string;
  profile_url: string;
  image_url: string;
}

table UsersStorage {
  users: [UserStorage];
}

root_type UsersStorage;
