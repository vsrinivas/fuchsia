{
    "program": {
        "binary": "test/trace_integration_tests"
    },
    "sandbox": {
        "features": [
            "persistent-storage",
            "system-temp"
        ],
	"services": [
            "fuchsia.process.Launcher",
            "fuchsia.sys.Launcher",
            "fuchsia.tracelink.Registry",
            "fuchsia.tracing.TraceController"
        ],
        "system": [ "bin" ]
    },
    "facets": {
        "fuchsia.test": {
            "injected-services": {
                "fuchsia.tracelink.Registry": [ "trace_manager", "--verbose=2" ],
                "fuchsia.tracing.TraceController": [ "trace_manager", "--verbose=2" ]
            }
        }
    }
}
