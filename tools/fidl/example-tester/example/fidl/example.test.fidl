library test.exampletester;

// This is an extremely simple FIDL protocol, with a single method that can be used to calculate
// the sum of two small positive numbers.
@discoverable
protocol Simple {
    /// Adds two unsigned integers together.
    Add(struct {
        augend uint8;
        addend uint8;
    }) -> (struct {
        sum uint16;
    });
};
