library fidl.test.enum;

type MyStrictEnum = strict enum : uint32 {
    FOO = 1;
    BAR = 2;
};

type MyFlexibleEnum = flexible enum : uint32 {
    FOO = 1;
    BAR = 2;
};

type MyFlexibleEnumWithCustomUnknown = flexible enum : uint32 {
    FOO = 1;
    BAR = 2;
    @unknown
    CUSTOM_UNKNOWN = 3;
};
