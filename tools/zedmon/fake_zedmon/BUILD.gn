import("//build/go/go_binary.gni")
import("//build/go/go_library.gni")

go_library("lib") {
  sources = [ "fake_zedmon.go" ]
}

go_binary("bin") {
  library = ":lib"
  output_name = "fake_zedmon"
}

group("fake_zedmon") {
  deps = [ ":bin" ]
}
