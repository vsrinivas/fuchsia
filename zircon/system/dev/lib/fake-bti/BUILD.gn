zx_library("fake-bti") {
  sdk = "shared"
  sdk_headers = [ "lib/fake-bti/bti.h" ]
  testonly = true
  shared = true
  sources = [
    "fake-bti.cc",
  ]
  deps = [
    "$zx/system/ulib/fbl",
    "$zx/system/ulib/fdio",
    "$zx/system/ulib/zircon",
    "$zx/system/ulib/zircon-internal",
    "$zx/system/ulib/zx",
  ]
  configs += [ "$zx_build/public/gn/config:static-libc++" ]
}
