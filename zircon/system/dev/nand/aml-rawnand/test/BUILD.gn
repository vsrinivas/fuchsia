group("test") {
  testonly = true
  deps = [ ":aml-rawnand-unittest" ]
}

zx_test("aml-rawnand-unittest") {
  include_dirs = [ ".." ]
  sources = [
    "../aml-rawnand.cc",
    "aml-rawnand-test.cc",
  ]
  deps = [
    "$zx/system/dev/lib/fake-bti",
    "$zx/system/dev/lib/fake_ddk",
    "$zx/system/dev/lib/mock-mmio-reg",
    "$zx/system/ulib/fdio",
    "$zx/system/ulib/zxtest",
    "..:common",
  ]
}
