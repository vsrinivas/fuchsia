zx_driver("nvme") {
  sources = [ "nvme.c" ]
  deps = [
    "$zx/system/banjo/ddk.protocol.block",
    "$zx/system/banjo/ddk.protocol.pci",
    "$zx/system/dev/lib/device-protocol-pci",
    "$zx/system/ulib/ddk",
    "$zx/system/ulib/sync",
    "$zx/system/ulib/zircon",
