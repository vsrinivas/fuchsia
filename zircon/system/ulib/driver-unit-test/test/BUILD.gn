group("test") {
  testonly = true
  deps = [ ":logger-unit" ]
}

zx_test("logger-unit") {
  sources = [ "logger-test.cc" ]
  deps = [
    "$zx/system/fidl/fuchsia-driver-test:c",
    "$zx/system/ulib/driver-unit-test",
    "$zx/system/ulib/fdio",
    "$zx/system/ulib/fidl",
    "$zx/system/ulib/zxtest",
  ]
}
