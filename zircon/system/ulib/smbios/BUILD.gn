zx_library("smbios") {
  sdk = "source"
  sdk_headers = [ "lib/smbios/smbios.h" ]
  kernel = true
  static = true
  sources = [ "smbios.cc" ]
  deps = [ "$zx/system/ulib/fbl" ]
}

zx_test("smbios-test") {
  sources = [ "smbios-test.cc" ]
  deps = [
    ":smbios",
    "$zx/system/ulib/fdio",
    "$zx/system/ulib/zxtest",
  ]
}
