group("test") {
  testonly = true
  deps = [ ":debugdata" ]
}

zx_test("debugdata") {
  sources = [ "debugdata.cc" ]
  deps = [
    "$zx/system/ulib/async-loop:async-loop-cpp",
    "$zx/system/ulib/async-loop:async-loop-default.static",
    "$zx/system/ulib/debugdata",
    "$zx/system/ulib/fidl-async:fidl-async-cpp",
    "$zx/system/ulib/fs",
    "$zx/system/ulib/zx",
    "$zx/system/ulib/zxtest",
  ]
}
