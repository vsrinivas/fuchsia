group("test") {
  testonly = true
  deps = [ ":range" ]
}

zx_test("range") {
  test_group = "sys"
  sources = [
    "interval-tree-test.cc",
    "range.cc",
  ]
  deps = [
    "$zx/system/ulib/fbl",
    "$zx/system/ulib/fdio",
    "$zx/system/ulib/range",
    "$zx/system/ulib/zxtest",
  ]
}
