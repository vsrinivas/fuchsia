import("//build/unification/zx_library.gni")

zx_library("fs-host") {
  sdk = "source"
  sdk_headers = [
    "fs-host/common.h",
    "fs-host/json_recorder.h",
  ]
  sources = [
    "common.cc",
    "json_recorder.cc",
  ]
  public_deps = [
    # <fs-host/json_recorder.h> has #include <lib/zircon-internal/thread_annotations.h>.
    "//zircon/public/lib/zircon-internal",
  ]
  deps = [ "//zircon/public/lib/fbl" ]
}
