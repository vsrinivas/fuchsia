group("test") {
  testonly = true
  deps = [ ":gpt-unit" ]
}

zx_test("gpt-unit") {
  sources = [
    "gpt-tests.cc",
    "main.cc",
  ]
  deps = [
    "$zx/system/fidl/fuchsia-hardware-block:c",
    "$zx/system/ulib/fbl",
    "$zx/system/ulib/fdio",
    "$zx/system/ulib/fdio-caller",
    "$zx/system/ulib/fzl",
    "$zx/system/ulib/gpt",
    "$zx/system/ulib/ramdevice-client",
    "$zx/system/ulib/zircon",
    "$zx/system/ulib/zx",
    "$zx/system/ulib/zxtest",
    "$zx/third_party/ulib/cksum",
  ]
}
