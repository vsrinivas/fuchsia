import("$zx_build/public/gn/migrated_targets.gni")

zx_library("ffl") {
  sdk = "source"
  sdk_headers = [
    "ffl/expression.h",
    "ffl/fixed_format.h",
    "ffl/fixed.h",
    "ffl/saturating_arithmetic.h",
    "ffl/string.h",
    "ffl/utility.h",
  ]
  sdk_migrated = true
  sources = []
  host = true
  kernel = true
  static = true
}

group("tests") {
  testonly = true
  deps = [ "test:tests" ]
}
