group("test") {
  testonly = true
  deps = [
    ":disk-inspector-unit",
  ]
}

test("disk-inspector-unit") {
  sources = [
    "basic-types-test.cpp",
  ]
  deps = [
    "$zx/system/ulib/disk-inspector",
    "$zx/system/ulib/fdio",
    "$zx/system/ulib/zircon",
    "$zx/system/ulib/zx",
    "$zx/system/ulib/zxtest",
  ]
  configs += [ "$zx/public/gn/config:static-libc++" ]
}
