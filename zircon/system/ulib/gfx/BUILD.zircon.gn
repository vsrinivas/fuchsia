import("$zx/public/gn/fuzzer.gni")

zx_library("gfx") {
  sdk = "source"
  sdk_headers = [ "gfx/gfx.h" ]
  sources = [ "gfx.c" ]
  deps = [ "$zx/system/ulib/zircon" ]

  # TODO(fxb/58162): delete the below and fix compiler warnings
  configs += [ "$zx_build_config:Wno-conversion" ]
}
