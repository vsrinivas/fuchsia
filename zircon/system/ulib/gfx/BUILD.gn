import("//build/zircon/migrated_targets.gni")

zx_library("gfx") {
  sdk = "source"
  sdk_headers = [ "gfx/gfx.h" ]

  if (is_efi_toolchain) {
    sources = []
  } else {
    sources = [ "gfx.c" ]
    deps = [ "//src/zircon/lib/zircon" ]
  }
}

group("tests") {
  testonly = true
  deps = [ "test:tests" ]
}
