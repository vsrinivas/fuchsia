group("test") {
  testonly = true
  deps = [ ":mock-function-test" ]
}

zx_test("mock-function-test") {
  output_name = "mock-function-test"
  sources = [ "mock-function-test.cc" ]
  deps = [
    "$zx/system/ulib/fbl",
    "$zx/system/ulib/fdio",
    "$zx/system/ulib/mock-function",
    "$zx/system/ulib/zircon",
    "$zx/system/ulib/zx",
    "$zx/system/ulib/zxtest",
  ]
}
