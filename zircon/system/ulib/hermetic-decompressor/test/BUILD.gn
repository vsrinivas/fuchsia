group("test") {
  testonly = true
  deps = [ ":hermetic-decompressor" ]
}

zx_test("hermetic-decompressor") {
  sources = [ "test.cc" ]
  deps = [
    "$zx/system/ulib/hermetic-decompressor",
    "$zx/system/ulib/zxtest",
    "$zx/third_party/ulib/lz4",
    "$zx/third_party/ulib/zstd",
  ]
}
