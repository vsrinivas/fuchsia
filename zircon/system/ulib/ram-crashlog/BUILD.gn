import("//build/zircon/migrated_targets.gni")

group("ram-crashlog") {
  public_deps = [ ":lib" ]
}

group("tests") {
  testonly = true
  deps = [ "tests" ]
}

zx_library("lib") {
  sdk = "source"
  sdk_headers = [ "ram-crashlog/ram-crashlog.h" ]

  sources = [ "ram-crashlog.cc" ]
  deps = [ "//zircon/third_party/ulib/cksum" ]
  kernel = true
  static = true
}
