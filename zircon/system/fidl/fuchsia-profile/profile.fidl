library fuchsia.scheduler;

using zx;

[Discoverable, Layout = "Simple"]
protocol ProfileProvider {
    // Obtain a profile handle.
    GetProfile(uint32 priority, string:64 name) -> (zx.status status, handle<profile>? profile);
};
