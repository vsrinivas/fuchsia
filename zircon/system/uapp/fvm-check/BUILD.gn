executable("fvm-check") {
  sources = [
    "main.cpp",
  ]
  deps = [
    "$zx/system/ulib/digest",
    "$zx/system/ulib/fbl",
    "$zx/system/ulib/fdio",
    "$zx/system/ulib/fvm",
    "$zx/system/ulib/fzl",
    "$zx/system/ulib/gpt",
    "$zx/system/ulib/ramdevice-client",
    "$zx/system/ulib/zircon",
    "$zx/system/ulib/zx",
    "$zx/third_party/ulib/cksum",
    "$zx/third_party/ulib/uboringssl",
  ]
  configs += [ "$zx/public/gn/config:static-libc++" ]
}
