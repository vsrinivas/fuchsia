import("$zx/kernel/params.gni")

source_set("bad-syscall") {
  testonly = true
  sources = [
    "bad-syscall.cc",
    "syscall.S",
  ]
  include_dirs = [ "$zx/kernel/arch/$zircon_cpu/include" ]
  deps = [
    "$zx/kernel/syscalls:syscall-abi",
    "$zx/system/ulib/fdio",
    "$zx/system/ulib/zircon",
    "$zx/system/ulib/zxtest",
  ]
}
