source_set("dirent") {
  deps = [ "$zx/third_party/ulib/musl:musl_internal" ]
  sources = [
    "alphasort.c",
    "scandir.c",
    "versionsort.c",
  ]

  if (zx == "/" || zircon_toolchain != false) {
    configs -= [ "$zx_build_config:visibility_hidden" ]
  }
}
