source_set("mman") {
  deps = [ "$zx/third_party/ulib/musl:musl_internal" ]
  sources = [
    "madvise.c",
    "mlock.c",
    "mlockall.c",
    "mmap.c",
    "mprotect.c",
    "msync.c",
    "munlock.c",
    "munlockall.c",
    "munmap.c",
    "posix_madvise.c",
    "shm_open.c",
  ]

  configs -= [ "$zx_build_config:visibility_hidden" ]
}
