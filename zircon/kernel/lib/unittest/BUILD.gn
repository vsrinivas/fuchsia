zx_library("unittest") {
  kernel = true
  sources = [
    "unittest.cc",
    "user_memory.cc",
  ]
  public_deps = [
    # <lib/unittest/user_memory.h> has #include <lib/crypto/prng.h>.
    "$zx/kernel/vm:headers",
  ]
  deps = [ "$zx/kernel/lib/console" ]
}
