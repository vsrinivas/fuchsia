library zx;

type GetStuffRequestArgs = struct {
    an_input int32;
    input2 int32;
};
type GetStuffResponseArgs = struct {};

@transport("Syscall")
protocol Couple {
    /// This does a single thing.
    DoThing(struct {
        an_input int32;
    }) -> () error status;

    /// Does great stuff.
    GetStuff(GetStuffRequestArgs) -> (GetStuffResponseArgs) error status;
};
