// TODO(fxb/39732): This should be read as "library zx".
library zz;

[Transport = "Syscall"]
protocol cache {
    /// Flush CPU data and/or instruction caches.
    [vdsocall]
    cache_flush(const_voidptr addr, usize size, uint32 options) -> (status status);
};
