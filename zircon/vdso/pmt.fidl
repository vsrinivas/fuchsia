library zx;

[Transport = "Syscall"]
protocol pmt {
    // TODO(fxbug.dev/32803): handle ZX_OBJ_TYPE_PMT; No rights required?
    /// Unpin pages and revoke device access to them.
    pmt_unpin(handle:PMT handle) -> (status status);
};
