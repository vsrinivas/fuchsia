import("//packages/package.gni")

executable("wget_bin") {
  output_name = "wget"

  sources = [
    "wget.cc",
  ]

  deps = [
    "//garnet/public/lib/app/cpp",
    "//garnet/public/lib/network/fidl",
    "//garnet/public/lib/fidl/cpp/bindings",
    "//garnet/public/lib/fxl",
    "//garnet/public/lib/fsl",
  ]
}

package("wget") {
  deps = [ ":wget_bin" ]

  binaries = [
    {
      name = "wget"
    },
  ]
}
